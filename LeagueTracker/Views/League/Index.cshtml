@model IEnumerable<Club>
@{
}

<!DOCTYPE html>

<html>
<head>
    <title>LeagueTracker</title>
</head>
<body>
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Klub</th>
        <th scope="col">WM</th>
        <th scope="col">RM</th>
      </tr>
    </thead>
    <tbody>
    @foreach (var club in Model)
    {
      <tr>
        <th scope="row">@club.Id</th>
        <td>@club.Name</td>
        <td>@club.DateOfCreation</td>
        <td>-</td>
      </tr>
    }
    </tbody>
  </table>
</body>
</html>